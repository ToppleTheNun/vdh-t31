{
  "name": "vdh-t31",
  "type": "module",
  "author": "Richard Harrah",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@8.6.8",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsx scripts/generate-env.ts && remix build --sourcemap",
    "dev": "tsx scripts/generate-env.ts && remix dev",
    "gql": "nr gql:introspection && nr gql:generate",
    "gql:generate": "graphql-codegen --config codegen.yml && prettier --write app/wcl/types.ts && prettier --write schema.json && prettier --write app/wcl/gql/schema.graphql",
    "gql:introspection": "tsx -r dotenv/config app/wcl/queries/introspection.ts",
    "lint": "eslint app && prettier -c app",
    "lint:fix": "eslint --fix app && prettier --write app",
    "start": "tsx scripts/generate-env.ts && remix-serve build",
    "typecheck": "tsc",
    "vercel-build": "nr build && sentry-upload-sourcemaps && rimraf build/*.map && rimraf public/build/*.map && rimraf public/build/**/*.map"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.1",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-slot": "1.0.2",
    "@remix-run/css-bundle": "2.0.1",
    "@remix-run/node": "2.0.1",
    "@remix-run/react": "2.0.1",
    "@remix-run/router": "1.9.0",
    "@remix-run/serve": "2.0.1",
    "@sentry/remix": "7.70.0",
    "@upstash/redis": "1.22.0",
    "@vercel/analytics": "1.0.2",
    "@vercel/remix": "2.0.1",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "date-fns": "2.30.0",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "graphql-tag": "2.12.6",
    "isbot": "3.7.0",
    "kleur": "4.1.5",
    "lucide-react": "0.279.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.46.2",
    "tailwind-merge": "1.14.0",
    "tailwindcss-animate": "1.0.7",
    "tiny-invariant": "1.3.1",
    "web-vitals": "3.4.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@antfu/ni": "0.21.8",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-graphql-request": "5.0.0",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@remix-run/dev": "2.0.1",
    "@remix-run/eslint-config": "2.0.1",
    "@types/node": "^18",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@vitejs/plugin-react": "4.0.4",
    "autoprefixer": "10.4.16",
    "eslint": "8.50.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "fontpie": "0.3.0",
    "jsdom": "22.1.0",
    "ofetch": "1.3.3",
    "prettier": "3.0.3",
    "prettier-plugin-pkg": "0.18.0",
    "prettier-plugin-tailwindcss": "0.5.4",
    "rimraf": "5.0.1",
    "tailwindcss": "3.3.3",
    "tsconfig-paths": "4.2.0",
    "tsx": "3.13.0",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vitest": "0.34.5",
    "vitest-tsconfig-paths": "3.4.1"
  },
  "sideEffects": false,
  "graphql": {
    "schema": "./schema.json"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-pkg",
      "prettier-plugin-tailwindcss"
    ]
  }
}
